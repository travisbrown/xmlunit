<ivy-module version="1.3">
  <info organization="@GROUP@"
    module="@ARTIFACT@"
    revision="@VERSION@"
    publication="@DATE@">
    <license name="@LICENSE@"
      url="@LICENSE_URL@"/>
    <description>@DESCRIPTION@</description>
  </info>
  <publications>
    <artifact name="@ARTIFACT@" type="@TYPE@"/>
  </publications>
</ivy-module>
